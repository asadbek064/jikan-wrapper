{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "rules": {
        "variable-name": {
            "options": [
                "ban-keywords",
                "check-format",
                "allow-pascal-case",
                "allow-leading-underscore",
                "allow-snake-case",
                "allow-trailing-underscore",
                "require-const-for-all-caps"
            ]
          },
          "array-type": false,
          "no-unused-expression": false
      }
}